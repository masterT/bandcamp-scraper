{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "tag-result",
    "type": "object",
    "additionalProperties": false,
    "description": "The JSON schema that matches a tag result.",
    "id": "https://mastert.github.io/bandcamp-scraper/schemas/tag-result.json",
    "properties": {
        "name": {
            "type": "string"
        },
        "artist": {
            "type": "string"
        },
        "url": {
            "type": "string",
            "format": "uri"
        },
        "artist_url": {
            "type": "string",
            "format": "uri"
        }
    },
    "required": [
        "name",
        "artist",
        "url",
        "artist_url"
    ]
}